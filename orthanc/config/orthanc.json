    "Database" : "orthanc",
    "Username" : "orthanc",
    "Password" : "orthanc"
  },
  "Plugins": [
    "/usr/local/share/orthanc/plugins"
  ],
  "Authorization": {
    "CheckedLevel": "studies"
  },
  "Worklists": {
    "Enable": true,
    "Database": "/var/lib/orthanc/worklists",
    "FilterIssuerAet": false,
    "LimitAnswers": 0
  }
}
